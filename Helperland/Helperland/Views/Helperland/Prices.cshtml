
@section css{
    <link rel="stylesheet" href="~/css/price.css" />
}

<section>
    <div> <img src="~/image/price/main.png" class="bg" /> </div>

</section>
<!-- /////////////////// PRICE////////////////// -->
<section class="price">
    <div class="container">
        <div class="heading">
            <h2 class="text-center">Prices</h2>
        </div>
        <div class="d-flex justify-content-center star">
            <hr / class="side_hr"> <img src="~/image/price/star.png" />
            <hr / class="side_hr">
        </div>
        <div class="prCard">
            <div class="prHeading text-center">One Time</div>
            <div class="prText d-flex align-items-center justify-content-center">&euro;18<sub>/hr</sub></div>
            <div class="prCaption row">
                <div class="prCaptionItem  col-sm-8 col-md-7 col-11"> <img src="~/image/price/tickmark.svg" alt="tick" /> Lower prices </div>
                <div class="prCaptionItem  col-sm-8 col-md-7 col-11"> <img src="~/image/price/tickmark.svg" alt="tick" /> Easy online & secure payment </div>
                <div class="prCaptionItem  col-sm-8 col-md-7 col-11"> <img src="~/image/price/tickmark.svg" alt="tick" /> Easy amendment </div>
            </div>
        </div>
    </div>
    <div class="d-flex justify-content-center align-items-center">
        <hr / class="phr">
    </div>
</section>
<!-- /////////////////// SERVICE////////////////// -->
<section class="service">
    <div class="container">
        <div class="heading">
            <h2 class="text-center">Extra services</h2>
        </div>
        <div class="d-flex justify-content-center star">
            <hr / class="side_hr"> <img src="~/image/price/star.png" />
            <hr / class="side_hr">
        </div>
        <div class=" d-flex flex-wrap  align-items-center justify-content-between servicefeature">
            <div>
                <div class="serviceImage rounded-circle d-flex justify-content-center align-items-center "> <img src="~/image/price/cabinet.svg" alt="InsideCabinets" /> </div>
                <div class="serviceText text-center">Inside Cabinets</div>
                <div class="serviceTime text-center">30 minutes</div>
            </div>
            <div>
                <div class="serviceImage rounded-circle d-flex justify-content-center align-items-center "> <img src="~/image/price/fridge.svg" alt="InsideFridge" /> </div>
                <div class="serviceText text-center">Inside Fridge</div>
                <div class="serviceTime text-center">30 minutes</div>
            </div>
            <div>
                <div class="serviceImage rounded-circle d-flex justify-content-center align-items-center "> <img src="~/image/price/oven.svg" alt="InsideOven" /> </div>
                <div class="serviceText text-center">Inside Oven</div>
                <div class="serviceTime text-center">30 minutes</div>
            </div>
            <div>
                <div class="serviceImage rounded-circle d-flex justify-content-center align-items-center "> <img src="~/image/price/laundary.svg" alt="LaundryWash" /> </div>
                <div class="serviceText text-center">Laundry Wash & Dry</div>
                <div class="serviceTime text-center">30 minutes</div>
            </div>
            <div>
                <div class="serviceImage rounded-circle d-flex justify-content-center align-items-center "> <img src="~/image/price/window.svg" alt="Interiorwindows" /> </div>
                <div class="serviceText text-center">Interior Windows</div>
                <div class="serviceTime text-center">30 minutes</div>
            </div>
        </div>
    </div>
</section>
